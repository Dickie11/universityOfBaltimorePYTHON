import csv
#changed file name after saving the cvs file in the same directory as the program .py file
tf= open('Baltimore_City_Employee_Salaries_FY2014.csv')
csv_tf = csv.reader(tf)
for row in csv_tf:
  print (row,'\n')

# the program opens the cvs file then reads the row of cvs data then prints all of it.

